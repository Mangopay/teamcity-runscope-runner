  <info>
    <name>runscope-runner</name>
    <display-name>Runscope support</display-name>
    <version>@Version@</version>
    <description>Support for Runscope tests</description>
    <download-url>https://github.com/Mangopay/teamcity-runscope-runner/releases</download-url>
